<% include ../partials/header.ejs %>

<h1 class="">Liked Posts</h1>

<div class="mainWrapper">

    <div class="formTop"><h1>Liked Posts</h1></div>

    <hr>
    
    <div class="gallery">
        <% user.likedPosts.forEach(function(post){ %>
        <div class="galleryPhotoContainer">
            <a href="/<%= post.author.username %>/post/<%= post._id %>">
                <img src="<%= post.image %>" alt="" class="galleryPhoto">
            </a>
        </div>
        <%}) %>
    </div>

</div>

<% include ../partials/footer.ejs %>